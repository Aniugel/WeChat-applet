.imageBg{
  width: 100%;
  height: 100%;
}
.btn{
  line-height: 40px;
  color: #fff;
  border-radius: 40px;
  text-align: center;
  background-color: #4dc1ad;
  position: fixed;
  top: 70px;
  left: 90px;
  right: 90px;
}
.box{
  position: absolute;
  top: 10px;
  left: 10px;
  right: 10px;
  bottom: 10px;
}
.box .list{
  margin-bottom: 10px;
  position: relative;
  height: 140px;
}
.box .list>image{
  position: absolute;
  z-index: -1;
  top: 0;
  width: 100%;
  height: 140px;
  border-radius: 4px;
}
.box navigator{
  height: 96px;
}
.box navigator text{
  display: block;
  margin-left: 14px;
  font-size: 18px;

}
.box navigator image{
  width: 44px;
  height: 44px;
  margin: 10px 10px 5px 10px;
}
.box .switchBox{
  width: 100%;
  height: 44px;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom-left-radius: 4px;
  border-bottom-right-radius: 4px;
  background-color: rgba(255, 255, 255, 0.6);
}
.box .switchBox image{
  width: 20px;
  height: 20px;
  margin-right: 10px;
}
.box .switchBox text{
  color: #9b9b9b;
}
.on{
  color: #4DC1AD !important;
}
.of{
  color: #9b9b9b !important;  
}