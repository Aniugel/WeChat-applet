page{
  background-color: #f7f7f7;
}
.user{
  height: 200px;
}
.user .bg{
  width: 100%;
  height: 200px;
  -moz-filter: blur(4px);
  -o-filter: blur(4px);
  -ms-filter: blur(4px);
  filter: blur(4px);
}
.user .img{
  width: 64px;
  height: 64px;
  position: absolute;
  top: 30px;
  left: 50%;
  margin-left: -32px;
}
.user text{
  position: absolute;
  top: 115px;
  left: 0;
  width: 100%;
  text-align: center;
}
.list{
  background-color: #fff;
  margin-top: 20px;
}
.list navigator .box{
  height: 44px;
  margin-left: 20px;
  border-bottom: 1px solid #eee;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.list navigator .rightArrown{
  width: 6px;
  height: 11px;
  margin-right: 20px;
}
.list navigator view{
  height: 44px;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.list navigator view image{
  width: 20px;
  height: 20px;
  margin-right: 10px;
}
.list navigator view .language{
  font-size: 14px;
  color: #4dc1ad;
  margin-right: 10px;
}
.quit{
  line-height: 44px;
  color: #d0021b;
  text-align: center;
  font-size: 16px;
  margin-top: 20px;
  background-color: #fff;
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
}
.version{
    display: block;
    width: 100%;
    text-align: center;
    margin-top: 40px;
    margin-bottom: 10px;
    font-size: 12px;
    color: #CCCCCC;
}
