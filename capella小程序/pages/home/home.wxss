.header{

  overflow: hidden;
  padding: 10px;
  background-color: #fff;
  margin-top: 10px;
  margin-bottom: 10px;
}
.header image{
  width: 160px;
  height: 50px;  
  float: left;
  margin-right: 20px;
}
.list{
  display: flex;
  flex-wrap: wrap;
}
.list .item{
  height: 90px;
  width: 33.33%;
  background-color: #fff;
  border-right: 1px solid #eee;
  border-bottom: 1px solid #eee;
  box-sizing: border-box;
  text-align: center;
}
.list .item image{
  margin-top: 15px;
  width: 30px;
  height: 30px;
}
.list .item text{
  display: block;
}
